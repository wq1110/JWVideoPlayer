<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="m_play_style_progressBar_custom_drawable" parent="@android:style/Widget.ProgressBar.Small">
        <item name="android:indeterminateDrawable">@drawable/m_play_ic_progress_loading_circle_draw</item>
        <item name="android:minWidth">25dp</item>
        <item name="android:minHeight">25dp</item>
        <item name="android:maxWidth">60dp</item>
        <item name="android:maxHeight">60dp</item>
    </style>
    <style name="m_play_style_SeekBarAppTheme">
        <item name="android:indeterminateOnly">false</item>
        <item name="android:focusable">true</item>
        <item name="android:progressDrawable">@drawable/m_play_bg_progress_drawable</item>
        <item name="android:indeterminateDrawable">@drawable/m_play_bg_progress_drawable</item>
        <item name="android:thumb">@drawable/m_play_bg_selector_seekbar_thumb</item>
        <item name="android:minHeight">3dp</item>
        <item name="android:maxHeight">3dp</item>
        <item name="android:paddingLeft">5dp</item>
        <item name="android:paddingRight">5dp</item>
    </style>
</resources>